<launch>
	
	<node name="map_server" pkg="map_server" type="map_server" args="$(find heros_2dnav)/map.pgm 0.05" />


	<node pkg="amcl" type="amcl" name="amcl" output="screen">

		<rosparam file="$(find heros_2dnav)/amcl_params.yaml" command="load" />

	</node>


	<node pkg="move_base" type="move_base" respawn="false" name="move_base" output="screen">
	
		<rosparam file="$(find heros_2dnav)/costmap_common_params.yaml" command="load" ns="global_costmap" />
		<rosparam file="$(find heros_2dnav)/costmap_common_params.yaml" command="load" ns="local_costmap" />
		<rosparam file="$(find heros_2dnav)/local_costmap_params.yaml" command="load" />
		<rosparam file="$(find heros_2dnav)/global_costmap_params.yaml" command="load" />
		<rosparam file="$(find heros_2dnav)/base_local_planner_params.yaml" command="load" />
		
	</node>
	
</launch>